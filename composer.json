{
    "name": "charliepage88/timezoned",
    "description": "Laravel trait for converting Eloquent model timestamps according to timezones",
    "keywords": ["laravel", "eloquent", "model", "timezone"],
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.7",
        "nesbot/carbon": "^1.37"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Weber",
            "email": "ryan@picoprime.com"
        },
	{
	    "name": "Charlie Page",
	    "email": "charliepage88@gmail.com"
	}
    ],
    "autoload": {
        "psr-4": {
            "RyanWeber\\Mutators\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RyanWeber\\Mutators\\TimezonedServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
